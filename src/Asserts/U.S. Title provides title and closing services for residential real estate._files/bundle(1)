        e.preventDefault();
        // YouTube part
        $('.videoModal').modal('show').find('.modal-body')
            .html('<div class="holds-the-iframe"><iframe style="width:100%;" src="https://www.youtube.com/embed/' + $(this).attr('href') + '" frameborder="0" title="YouTube Video embeded into the page" /></div>');
    });

