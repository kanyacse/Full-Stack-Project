                    });
            },
            remove: function () {
                return maskScope.call(this, {
                    action: "remove"
                });
            },
            getemptymask: function () {
                return this.maskset = this.maskset || generateMaskSet(this.opts, this.noMasksCache),
                    maskScope.call(this, {
                        action: "getemptymask"
                    });
            },
            hasMaskedValue: function () {
                return !this.opts.autoUnmask;
            },
