                case "mask":
                    !function (elem) {
                        EventRuler.off(elem);
                        var isSupported = function (input, opts) {
                            var elementType = input.getAttribute("type"), isSupported = "INPUT" === input.tagName && -1 !== $.inArray(elementType, opts.supportsInputType) || input.isContentEditable || "TEXTAREA" === input.tagName;
